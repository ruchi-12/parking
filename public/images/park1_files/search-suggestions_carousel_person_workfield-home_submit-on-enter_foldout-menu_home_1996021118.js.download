(function($) {
    'use strict';

    $.widget("ui.workfield_home", {

        _init: function() {
            this.init();
        },

        init: function() {
            var me = this,
                element = me.element,
                button = element.find('.home__project__workfield__btn');

            me.modalWrapper = element.find('.modal-wrapper--home');
            me.modalContent = me.modalWrapper.find('.workfield__content');
            me.closeButton = element.find('.modal-close');

            button.on('click', me.openModal.bind(me));
            me.document.on('click touchstart', me._handleDocumentClick.bind(me));
        },

        openModal: function(){
        	var me = this,
                isAndroid = /(android)/i.test(navigator.userAgent),
                iScrollConfig = {
                    mouseWheel: true,
                    wheelAction: 'scroll',
                    scrollbars: false,
                    bounce: false,
                    keyBindings: false,
                    probeType: 2,
                    preventDefault: true,
                    tap: true,
                    click: true
                };

        	me.modalWrapper.removeClass('is-hidden');
        	me.modalContent.removeClass('is-hidden');

            $('body').addClass('is-modal-open');

            if (isAndroid) {
                iScrollConfig.disablePointer = true;
                iScrollConfig.disableTouch = false;
            }

            me.iScroll = new IScroll('#' + me.modalWrapper.attr('id'), iScrollConfig);

        	return false;
        },

        closeModal: function(){
        	var me = this;

        	me.modalWrapper.addClass('is-closing');

            setTimeout(function(){
                me.modalWrapper.removeClass('is-closing');
                me.modalWrapper.addClass('is-hidden');
                me.modalContent.addClass('is-hidden');    

                $('body').removeClass('is-modal-open');

                if (me.iScroll) {
                    me.iScroll.destroy();
                }

            }, 300);


            // Didn't work on ie11 please uncomment when ie11 is out of the picture and destroy that ugly timeout
            
            // if (me.modalContent.hasClass('is-closing')) {
            //     me.modalContent.one('webkitAnimationEnd oanimationend MSAnimationEnd animationend', function () {
            //                     me.modalContent.removeClass('is-closing');
            //                     me.modalWrapper.addClass('is-hidden');
            //                     me.modalContent.addClass('is-hidden');
            //                 });
            // }
        },

         _handleDocumentClick: function(event) {

            event.stopPropagation(); 

            var $target = $(event.originalEvent.target),
                me = this,
                isOpen = !me.modalWrapper.hasClass('is-hidden');

            // Clicked on/in modal close button
            if ($target.closest(me.closeButton).size() > 0 && isOpen) {
                me.closeModal(event);
                event.preventDefault();
                return;
            }

            //Clicked outside of modalContent
            if ($target.closest(me.modalContent).size() == 0 && isOpen) {
                me.closeModal(event);
                event.preventDefault();
                return;
            }
        },

    });
})(jQuery);
